{
  "manifest_version": 3,
  "name": "My PR Viewer",
  "version": "1.0",
  "description": "List all open PR for a user",
  "permissions": [
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://api.github.com/",
    "https://github.cloud.capitalone.com/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}